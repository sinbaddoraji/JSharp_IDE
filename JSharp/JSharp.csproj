    <Content Include="Resources\Java-LightMode.xshd">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
    <Content Include="Resources\Python-DarkMode.xshd">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
    <Content Include="Resources\Python-LightMode.xshd">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
