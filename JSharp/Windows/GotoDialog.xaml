<Window x:Class="JSharp.Windows.GotoDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:JSharp.Windows"
        mc:Ignorable="d"
        Title="GotoDialog" Height="150" Width="400">
    <Grid>
        <TextBox x:Name="txt" Text="2" HorizontalAlignment="Left" Margin="26,39,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Height="27" Width="338" BorderBrush="Black" KeyDown="Txt_KeyDown" />
        <Button Content="Goto" HorizontalAlignment="Left" Margin="151,84,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>
    </Grid>
</Window>
